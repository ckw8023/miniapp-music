page{
    padding: 10rpx;
    box-sizing: border-box;
}
.imgbox{
    background-color: yellow;
    height: 400rpx;
}

.imgbox image{
    width: 100%;
    height: 100%;
}
.imgbox text{
    margin: auto;
}

.title{
    margin-top: 60rpx;
    font-size: 32rpx;
    font-weight: bold;
    line-height: 50rpx;
    text-indent: 30rpx;
    border-left: 2px solid red;
    color:#333;
  }
